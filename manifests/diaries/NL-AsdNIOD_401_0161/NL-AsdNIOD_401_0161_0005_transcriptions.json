      ],
      "id": "https://amsterdamtimemachine.github.io/datasprint-annoteren-2024/manifests/diaries/NL-AsdNIOD_401_0161/NL-AsdNIOD_401_0161_0005_transcriptions.json/tr_1_tl_27",
      "type": "Annotation",
      "textGranularity": "line",
      "motivation": "supplementing",
      "body": [
        {
          "type": "TextualBody",
          "value": "zei: \"Houdt je maar flink, hoor!! \" En dat is 't laatste, wat ik"
        }
      ],
      "target": "https://amsterdamtimemachine.github.io/datasprint-annoteren-2024/manifests/diaries/NL-AsdNIOD_401_0161/NL-AsdNIOD_401_0161_0005#xywh=304,1651,1262,58"
    },
