      ],
      "id": "https://amsterdamtimemachine.github.io/datasprint-annoteren-2024/manifests/diaries/NL-AsdNIOD_401_0161/NL-AsdNIOD_401_0161_0005_transcriptions.json/tr_1_tl_17",
      "type": "Annotation",
      "textGranularity": "line",
      "motivation": "supplementing",
      "body": [
        {
          "type": "TextualBody",
          "value": "uit te kijken. Bij de Marinekantine ving ik het begin van de troep"
        }
      ],
      "target": "https://amsterdamtimemachine.github.io/datasprint-annoteren-2024/manifests/diaries/NL-AsdNIOD_401_0161/NL-AsdNIOD_401_0161_0005#xywh=288,1157,1326,50"
    },
    {
