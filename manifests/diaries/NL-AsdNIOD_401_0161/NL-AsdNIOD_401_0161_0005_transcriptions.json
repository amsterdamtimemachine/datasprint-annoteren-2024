      "type": "Annotation",
      "textGranularity": "line",
      "motivation": "supplementing",
      "body": [
        {
          "type": "TextualBody",
          "value": "een glad geschoren hoofd en een bril op met een sprongetje boven"
        }
      ],
      "target": "https://amsterdamtimemachine.github.io/datasprint-annoteren-2024/manifests/diaries/NL-AsdNIOD_401_0161/NL-AsdNIOD_401_0161_0005#xywh=294,1255,1284,50"
