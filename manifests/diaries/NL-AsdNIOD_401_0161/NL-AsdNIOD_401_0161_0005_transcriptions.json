      ],
      "id": "https://amsterdamtimemachine.github.io/datasprint-annoteren-2024/manifests/diaries/NL-AsdNIOD_401_0161/NL-AsdNIOD_401_0161_0005_transcriptions.json/tr_1_tl_18",
      "type": "Annotation",
      "textGranularity": "line",
      "motivation": "supplementing",
      "body": [
        {
          "type": "TextualBody",
          "value": "weer op en keek naar alle lange mannen. Ineens zie ik iemand met"
        }
      ],
      "target": "https://amsterdamtimemachine.github.io/datasprint-annoteren-2024/manifests/diaries/NL-AsdNIOD_401_0161/NL-AsdNIOD_401_0161_0005#xywh=294,1205,1282,52"
