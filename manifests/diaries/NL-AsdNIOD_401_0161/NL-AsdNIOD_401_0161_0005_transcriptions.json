      ],
      "id": "https://amsterdamtimemachine.github.io/datasprint-annoteren-2024/manifests/diaries/NL-AsdNIOD_401_0161/NL-AsdNIOD_401_0161_0005_transcriptions.json/tr_1_tl_23",
      "type": "Annotation",
      "textGranularity": "line",
      "motivation": "supplementing",
      "body": [
        {
          "type": "TextualBody",
          "value": "aan de kant, waar Papa liep en juist op de hoek. Een paar pas van"
        }
      ],
      "target": "https://amsterdamtimemachine.github.io/datasprint-annoteren-2024/manifests/diaries/NL-AsdNIOD_401_0161/NL-AsdNIOD_401_0161_0005#xywh=296,1452,1306,58"
    },
