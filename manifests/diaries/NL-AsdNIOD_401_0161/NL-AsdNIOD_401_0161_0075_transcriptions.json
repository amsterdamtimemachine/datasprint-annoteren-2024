      "type": "Annotation",
      "textGranularity": "line",
      "motivation": "supplementing",
      "body": [
        {
          "type": "TextualBody",
          "value": "sleurde ons hoog het water in en daar gingen wij - en waar wij zweefden"
        }
      ],
